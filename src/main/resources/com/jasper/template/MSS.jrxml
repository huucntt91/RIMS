	</parameter>
	<queryString>
		<![CDATA[select rownum, a.ma_node,
                             a.node_cha_id,
                             a.donvi_id,
                             a.thiet_bi_id,
                             a.building_id,
                             a.loai_tram_id,
                             a.ten_ng_qly,
                             a.sdt_ng_qly,
                             a.ne_type_id,
                             a.node_id nodeid,
                             parentusertype.ma_node ma_node_cha,
                             b.ma_building,
                             h.*,
                             oh.opc,
                             oh.opc1,
                             oh.numeral_system,
                             oh.numeral_system1
                        FROM rims.node a
                             LEFT JOIN rims.mss_info h ON a.node_id = h.node_id
                             LEFT JOIN rims.node parentusertype ON a.node_cha_id = parentusertype.node_id
                             LEFT JOIN rims.building b ON a.building_id = b.building_id
                             LEFT JOIN rims.oss_mss_info oh ON oh.node_id = h.node_id
                       WHERE 1 = 1 AND a.ne_type_id = 13
    AND ( lower(h.name)     like lower('%'||$P{name}||'%')
    OR NVL($P{name},'null') = 'null')]]>
	</queryString>
