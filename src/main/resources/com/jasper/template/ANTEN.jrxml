	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="prs_name" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="prs_khuvuc_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="prs_tinhtp_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select rownum, a.ten_anten,
                             a.hang_sx,
                             a.trong_luong,
                             a.kich_co,
                             a.do_cao_lap_dat,
                             a.loai_antena_id,
                             a.bang_tan_id,
                             a.cau_hinh_port_id,
                             a.cau_hinh_gain,
                             a.cau_hinh_beam_width,
                             g.node_id, g.ma_node, g.donvi_id, g.building_id,
       g.ne_type_id,  g.trang_thai_hd_id,
       g.trang_thai_ql_id,
       g.status, g.user_insert,
                             b.tinhtp_id,
                             b.dia_chi,
                             b.ma_building,
                             c.ten_loai_anten,
                             d.cau_hinh_port,
                             e.ten_bang_tan,
                             h.ten_tinh_tp
                        FROM rims.node g
                             left JOIN rims.anten_info a  ON a.node_id = g.node_id
                             LEFT JOIN rims.building b
                                 ON g.building_id = b.building_id
                             LEFT JOIN rims.loai_anten c
                                 ON a.loai_antena_id = c.loai_anten_id
                             LEFT JOIN rims.dm_cau_hinh_port d
                                 ON a.cau_hinh_port_id = d.cau_hinh_port_id
                             LEFT JOIN rims.dm_bang_tan e
                                 ON a.bang_tan_id = e.bang_tan_id
                             LEFT JOIN rims.tinh_tp h ON b.tinhtp_id = h.tinhtp_id
                             LEFT JOIN rims.dm_khu_vuc kv ON h.khu_vuc = kv.id
                       WHERE   g.ne_type_id = 21
                             and  (   LOWER (a.ten_anten) LIKE
                                         '%' || LOWER ($P{prs_name}) || '%'
                                  OR NVL ($P{prs_name}, '') = '')
                             AND (   $P{prs_tinhtp_id} IS NULL
                                  OR b.tinhtp_id IN (    SELECT REGEXP_SUBSTR (
                                                                    $P{prs_tinhtp_id},
                                                                    '[^,]+',
                                                                    1,
                                                                    LEVEL)
                                                                    ids
                                                           FROM DUAL
                                                     CONNECT BY REGEXP_SUBSTR (
                                                                    $P{prs_tinhtp_id},
                                                                    '[^,]+',
                                                                    1,
                                                                    LEVEL)
                                                                    IS NOT NULL))
                             AND (  $P{prs_khuvuc_id} IS NULL
                                  OR kv.id IN (    SELECT REGEXP_SUBSTR (
                                                              $P{prs_khuvuc_id},
                                                              '[^,]+',
                                                              1,
                                                              LEVEL)
                                                              ids
                                                     FROM DUAL
                                               CONNECT BY REGEXP_SUBSTR (
                                                              $P{prs_khuvuc_id},
                                                              '[^,]+',
                                                              1,
                                                              LEVEL)
                                                              IS NOT NULL))
                    ORDER BY g.node_id DESC]]>
	</queryString>
